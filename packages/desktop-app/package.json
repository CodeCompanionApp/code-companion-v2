{
  "name": "code-companion-desktop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.1",
    "electron": "1.7.9",
    "electron-devtools-installer": "2.2.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "globby": "^6.1.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "style-loader": "^0.19.0",
    "css-loader": "^0.28.7",
    "util.promisify": "1.0.0"
  },
  "scripts": {
    "build": "concurrently --kill-others \"npm run build-renderer\" \"npm run build-preload\"",
    "build-renderer": "webpack --config webpack.config.js --progress --colors",
    "build-preload": "webpack --config webpack.config.preload.js --progress --colors",
    "watch-preload": "webpack --config webpack.config.preload.js --colors --watch",
    "dev-server": "webpack-dev-server --config webpack.config.dev.js --colors",
    "electron-dev": "cross-env NODE_ENV=development electron -r babel-register ./main.js",
    "electron-prod": "cross-env NODE_ENV=production electron -r babel-register ./main.js",
    "start": "concurrently --kill-others \"npm run dev-server\" \"npm run electron-dev\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  }
}
