{
  "name": "code-companion-desktop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "electron": "^1.7.5",
    "globby": "^6.1.0",
    "history": "^4.7.2",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2"
  },
  "scripts": {
    "build": "concurrently --kill-others \"npm run build-renderer\" \"npm run build-preload\"",
    "build-renderer": "webpack --config webpack.config.js --progress --colors",
    "build-preload": "webpack --config webpack.config.preload.js --progress --colors",
    "watch-preload": "webpack --config webpack.config.preload.js --colors --watch",
    "dev-server": "webpack-dev-server --config webpack.config.dev.js --colors",
    "electron-dev": "cross-env NODE_ENV=development electron -r babel-register ./main.js",
    "electron-prod": "cross-env NODE_ENV=production electron -r babel-register ./main.js",
    "start": "concurrently --kill-others \"npm run dev-server\" \"npm run electron-dev\" \"npm run watch-preload\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.30.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
